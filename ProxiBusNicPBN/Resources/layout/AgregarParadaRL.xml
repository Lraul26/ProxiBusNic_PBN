<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@color/background"
    android:padding="15.0dp"
    android:layout_height="match_parent">

     <TextView
        android:gravity="center"
        android:textColor="@android:color/white"
        android:text="@string/txt15"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_width="match_parent"
        android:layout_height="78.5dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/txtTitulo" />
    <TextView
        android:textColor="@android:color/white"
        android:text="@string/txt18"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView8" />
    <EditText
        android:textColorHint="@color/blanco"
        android:hint="@string/txt19"
        android:textColor="@android:color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editDescripcion" />
    <TextView
        android:textColor="@android:color/holo_red_light"
        android:text="@string/txt20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView4" />
    <TextView
        android:text=""
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView10" />
    <TextView
        android:textColor="@android:color/white"
        android:text="@string/txt21"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView9" />
    <EditText
        android:textColor="@android:color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editAlias" />
     <TextView
        android:text="@string/txt22"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView5" />
    <TextView
        android:text=""
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView11" />
    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout4" >
        <TextView
            android:textColor="@android:color/white"
            android:text="@string/txt23"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="183.5dp"
            android:layout_height="match_parent"
            android:id="@+id/textView12" />
        <TextView
            android:textColor="@android:color/white"
            android:text="@string/txt24"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/textView13" />
    </LinearLayout>
    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout2">
        <EditText
            android:gravity="center"
            android:textColor="@android:color/white"
            android:inputType="textMultiLine"
            android:layout_width="183.5dp"
            android:layout_height="match_parent"
            android:id="@+id/editlatitud" />
       
        <EditText
            android:gravity="center"
            android:textColor="@android:color/white"
            android:inputType="textMultiLine"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/editlongitud" />
       
    </LinearLayout>
    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout3">
        <TextView
            android:text="@string/txt22"
            android:layout_width="183.5dp"
            android:layout_height="match_parent"
            android:id="@+id/textView6" />
        <TextView
            android:text="@string/txt22"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/textView7" />

    </LinearLayout>
    <TextView
        android:textColor="@android:color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Foto"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView3" />
    <ImageView
        android:contentDescription=""
        android:src="@drawable/paradapordefecto"
        android:layout_width="match_parent"
        android:layout_height="206.5dp"
        android:id="@+id/imageParada" />
    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="69.0dp"
        android:id="@+id/linearLayout1">
        <ImageButton
            android:contentDescription=""
            android:layout_margin="5dp"
            android:paddingBottom="5dp"
            android:background="@color/background"
            android:src="@drawable/galeria"
            android:layout_width="65.0dp"
            android:layout_height="74.0dp"
            android:id="@+id/btnGaleria" />
        <ImageButton
            android:contentDescription=""
            android:layout_margin="5dp"
            android:paddingBottom="5dp"
            android:background="@color/background"
            android:src="@drawable/camara"
            android:layout_width="51.0dp"
            android:layout_height="match_parent"
            android:id="@+id/btnCamara" />
    </LinearLayout>
    <ImageButton
        android:background="@color/background"
        android:src="@drawable/aceptar"
        android:layout_width="69.5dp"
        android:layout_height="wrap_content"
        android:id="@+id/btnAceptar" />
    <TextView
        android:gravity="center"
        android:textColor="@android:color/white"
        android:text="@string/txt29"
        android:layout_width="74.5dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView14" />
   
</LinearLayout>
